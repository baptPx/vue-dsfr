{
  "name": "@thales/vue-dsfr",
  "version": "1.0.57",
  "files": [
    "dist",
    "types",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.x.x",
    "npm": "8.x.x"
  },
  "types": "./types",
  "license": "MIT",
  "main": "./dist/vue-dsfr.umd.js",
  "module": "./dist/vue-dsfr.mjs",
  "exports": {
    ".": {
      "require": "./dist/vue-dsfr.umd.js",
      "import": "./dist/vue-dsfr.mjs"
    },
    "./dist/vue-dsfr.css": {
      "require": "./dist/vue-dsfr.css",
      "import": "./dist/vue-dsfr.css"
    },
    "./styles": {
      "require": "./dist/vue-dsfr.css",
      "import": "./dist/vue-dsfr.css"
    }
  },
  "publishConfig": {
    "registry": "http://localhost:8081/repository/npm-private"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "check-exports": "node ./ci/check-exports.mjs",
    "check-exports-ci": "node ./ci/check-exports.mjs --ci",
    "dev": "npm run storybook",
    "clean-dist": "rimraf dist",
    "prebuild": "run-s clean-dist",
    "build": "run-p build:css build:js",
    "postbuild": "copyfiles types/composable.d.ts dist/composable.d.ts",
    "build:css": "node build-css.mjs",
    "build:js": "cross-env NODE_ENV=production rollup -c",
    "demo-app": "vite",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "test": "run-s test:unit test:ct-ci",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run",
    "test:ct": "cypress open --component",
    "test:ct-ci": "cypress run --component",
    "format": "npm run lint -- --fix",
    "lint": "eslint ./src/**/*.{vue,js}",
    "storybook": "cross-env start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "preview": "run-s build-storybook",
    "view": "vite serve storybook-static",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "vite:build": "vite build && tsc -d --emitDeclarationOnly && vue-typegen gen -s src -o types",
    "typegen": "tsc -d --emitDeclarationOnly && vue-typegen gen -s src -o types",
    "vue-tsc": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.8.5",
    "focus-trap": "~6.7.0",
    "focus-trap-vue": "^3.3.1",
    "oh-vue-icons": "1.0.0-rc3",
    "rollup": "^3.3.0",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-env": "^7.19.1",
    "@cypress/vite-dev-server": "3.1.1",
    "@cypress/vue": "^4.2.0",
    "@mdx-js/react": "^1.6.22",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-url": "^7.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/client-api": "^6.5.12",
    "@storybook/client-logger": "^6.5.12",
    "@storybook/core-common": "^6.5.12",
    "@storybook/vue3": "^6.5.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^29.0.3",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.21",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "c8": "^7.12.0",
    "chalk": "^5.0.1",
    "copyfiles": "^2.4.1",
    "core-js": "^3.25.3",
    "cross-env": "^7.0.3",
    "cypress": "^10.8.0",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^8.24.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-vue": "^9.5.1",
    "globby": "^13.1.2",
    "inquirer": "^9.1.2",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "p-debounce": "^4.0.0",
    "postcss": "^8.4.16",
    "postcss-csso": "^6.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.0.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.8.2",
    "postcss-url": "^10.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.8.2",
    "rollup-plugin-vue": "^6.0.0",
    "semantic-release": "^19.0.5",
    "start-server-and-test": "^1.14.0",
    "tsd-jsdoc": "^2.5.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.23.4",
    "vue-loader": "^17.0.0",
    "vue-router": "^4.1.5",
    "vue-router-mock": "^0.1.9",
    "vue-tsc": "^0.40.13",
    "vue-typegen": "^0.2.0",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "oh-vue-icons": "1.0.0-rc3",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "optionalDependencies": {
    "esbuild-linux-64": "^0.15.9"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "pre-push": "npm test"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "run-s check-exports-ci lint --"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnum-mi/vue-dsfr.git"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true,
        "branch": "develop"
      }
    ],
    "assets": [
      "dist/*.js",
      "dist/*.css"
    ]
  }
}
